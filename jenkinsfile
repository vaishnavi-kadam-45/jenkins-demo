pipeline
{
agent any
  stages{
    stage("INstall git"){
    steps{
    sh 'sudo apt install git'
    }
    parallel {
    stage("Install Apache"){
      steps{
       sh 'sudo apt install apache2 -y'
      }
      }
     }
    }
   
  stage("Deploy"){
  steps{
   sh '''sudo cp -R * /var/www/html/
      sudo chmod -R 777 /var/www/html '''
  }
  }
}
}
