pipeline
{
agent any
  stages{
    stage("INstall somthing"){
    parallel {
    stage("Install Apache"){
      steps{
       sh 'sudo apt install apache2 -y'
      }
      }
     }
    }
    stage("Get My code"){
      steps{
git { credentialsId: '82bd8dc4-40e9-426d-a858-a74bee253b21', url: 'https://github.com/vaishnavi-kadam-45/jenkins-demo.git' }
   }
    
  }
  stage("Deploy"){
  steps{
   sh '''sudo cp -R * /var/www/html/
      sudo chmod R 777 /var/www/html '''
  }
  }
}
